{
  "name": "muhasel",
  "productName": "Muhasel",
  "private": true,
  "version": "1.0.0",
  "main": "dist-electron/main.cjs",
  "type": "module",
  "description": "Muhasel - School Finance Management System",
  "author": "Your Name",
  "scripts": {
    "dev": "node ensure-port.js && npx vite --port 5175 --host localhost --force",
    "clean": "rimraf dist dist-electron",
    "build": "npm run clean && cross-env NODE_ENV=production vite build && node copy-electron-files.js",
    "build:win": "npm run clean && set NODE_ENV=production && vite build && node copy-electron-files.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"npm run dev\" \"npx wait-on http://localhost:5175 && npm run electron\"",
    "electron:build": "npm run build && electron-forge make",
    "electron:package": "npm run build && electron-forge package",
    "electron:package:alt": "npm run build && electron-packager . Muhasel --platform=win32 --arch=x64 --asar --overwrite --out=release --app-copyright=\"Copyright © 2023 Muhasel\" --app-version=1.0.0 --executable-name=Muhasel --win32metadata.CompanyName=\"Muhasel Team\" --win32metadata.FileDescription=\"Muhasel Finance System\" --win32metadata.OriginalFilename=\"Muhasel.exe\" --win32metadata.ProductName=\"Muhasel\" --win32metadata.InternalName=\"Muhasel\" --win32metadata.RequestedExecutionLevel=asInvoker",
    "electron:package:test": "npm run build && electron-packager . MuhaselTest --platform=win32 --arch=x64 --overwrite --out=release-test --app-copyright=\"Copyright © 2023 Muhasel\" --app-version=1.0.0 --executable-name=MuhaselTest --win32metadata.CompanyName=\"Muhasel Team\" --win32metadata.FileDescription=\"Muhasel Finance System Test\" --win32metadata.OriginalFilename=\"MuhaselTest.exe\" --win32metadata.ProductName=\"MuhaselTest\" --win32metadata.InternalName=\"MuhaselTest\" --win32metadata.RequestedExecutionLevel=asInvoker",
    "electron:make": "npm run build && electron-forge make",
    "create-installer": "node windows-installer.cjs",
    "package-and-install": "npm run electron:package:alt && npm run create-installer",
    "start": "npm run electron:dev",
    "electron": "electron .",
    "electron:serve": "concurrently -k \"vite\" \"electron .\"",
    "electron:test-dist": "cross-env NODE_ENV=production electron --no-sandbox . --prod",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "generate-icons": "node generate-icons.js",
    "electron:builder": "npm run build && electron-builder --win",
    "build:installer": "npm run build && electron-builder --win nsis"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-select": "^2.2.5",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/supabase-js": "^2.50.4",
    "@types/file-saver": "^2.0.7",
    "@types/pdfmake": "^0.2.11",
    "arabic-reshaper": "^1.1.0",
    "axios": "^1.6.2",
    "bidi-js": "^1.0.3",
    "chart.js": "^4.4.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.6",
    "electron-log": "^5.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.2.1",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.1",
    "flowbite": "^2.2.0",
    "i18next": "^23.7.7",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "jotai": "^2.6.0",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "jwt-decode": "^3.1.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.244.0",
    "match-sorter": "^6.3.1",
    "mime": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.2",
    "pdfmake": "^0.2.20",
    "playwright": "^1.57.0",
    "random-words": "^2.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.5.0",
    "react-otp-input": "^3.1.1",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.8.0",
    "react-step-wizard": "^5.3.11",
    "react-table": "^7.8.0",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.3.3",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^8.41.0",
    "@fontsource/tajawal": "^5.2.7",
    "@types/cors": "^2.8.13",
    "@types/electron": "^1.4.38",
    "@types/express": "^4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/mime": "^3.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.14",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "electron": "^27.3.11",
    "electron-builder": "^26.0.12",
    "electron-icon-builder": "^2.0.1",
    "electron-icon-maker": "^0.0.5",
    "electron-packager": "^17.1.2",
    "electron-winstaller": "^5.4.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "globals": "^13.20.0",
    "kill-port": "^2.0.1",
    "postcss": "^8.4.31",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.4.19",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.muhasel.app",
    "productName": "Muhasel",
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "extraResources": [
      {
        "from": "dist",
        "to": "app/dist",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist-electron",
        "to": "app/dist-electron",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico",
      "executableName": "Muhasel",
      "verifyUpdateCodeSignature": false,
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Muhasel",
      "artifactName": "Muhasel-Setup-${version}.${ext}",
      "menuCategory": "Muhasel",
      "deleteAppDataOnUninstall": true,
      "displayLanguageSelector": false
    },
    "mac": {
      "category": "public.app-category.business",
      "target": [
        "dmg"
      ],
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office",
      "icon": "build/icon.png"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "Muhasel",
        "executableName": "muhasel",
        "asar": true,
        "icon": "dist/icons/icon",
        "extraResource": [
          "dist",
          "dist-electron"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Muhasel",
            "setupExe": "Muhasel-Setup.exe",
            "setupIcon": "dist/icons/icon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "icon": "dist/icons/icon.png"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
